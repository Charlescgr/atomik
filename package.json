{
  "name": "atomik-template",
  "version": "1.1.0",
  "private": true,
  "description": "A library to shared Atomik template Components.",
  "author": "Charles Rodrigues <charles.cgrod@gmail.com>",
  "contributors": [],
  "main": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm-run-all -s clean rollup:build",
    "build:storybook": "cross-env STORYBOOK=on && build-storybook -c .storybook -o build",
    "build:watch": "npm run clean && npm-run-all -p clean rollup:watch",
    "changelog": "cross-env auto-changelog && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && git push",
    "clean": "rimraf dist",
    "coverage": "cross-env NODE_ENV=test jest --coverage",
    "dev": "run-p rollup:watch example:watch",
    "example:install": "cd example && npm install",
    "example:lint": "eslint ./example --ext .js",
    "example:lint:fix": "eslint ./example --ext .js --fix",
    "example:build": "cd example && npm run build",
    "example:serve": "cd example && npm run start",
    "example:watch": "cd example && npm install && npm run dev",
    "example:css-validator": "cd example && npm run css-validator",
    "lint": "eslint ./src --ext .js",
    "lint:fix": "eslint ./src --ext .js --fix",
    "package:order": "npx sort-package-json",
    "prepare": "npm run build",
    "rollup:build": "rollup -c config/rollup.config.js",
    "rollup:watch": "rollup -c config/rollup.config.js -w",
    "serve:all": "concurrently \"npm run example:serve\" \"npm run serve:storybook\"",
    "storybook:serve": "cross-env STORYBOOK=on && http-server-spa ./build-storybook/ index.html ${SERVER_PORT:=9005}",
    "start": "npm-run-all -s rollup:watch example:watch",
    "storybook": "cross-env STORYBOOK=on && start-storybook -s ./example/public/ -p 6006 --no-dll",
    "storybook:build": "cross-env STORYBOOK=on && build-storybook -c .storybook -o ./build-storybook -s ./example/public/",
    "test": "cross-env CI=1 SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jsdom",
    "test:fix": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jsdom -u",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run example:lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/eslint-parser": "^7.11.3",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-syntax-export-default-from": "^7.10.4",
    "@babel/plugin-syntax-import-meta": "^7.10.1",
    "@babel/plugin-syntax-jsx": "^7.10.1",
    "@babel/plugin-transform-flow-strip-types": "^7.10.1",
    "@babel/plugin-transform-react-jsx": "^7.10.1",
